# Location of BUFG Primitives 
set_property LOC BUFGCTRL_X0Y17 [get_cells clkf_buf]
set_property LOC BUFGCTRL_X0Y16 [get_cells cpu_clk_buf]
set_property LOC BUFGCTRL_X0Y0 [get_cells SCLK_OBUF_BUFG_inst]

# Location of IO Primitives which is load of clock spine
set_property LOC IOB_X1Y86 [get_cells SCLK_OBUF_inst]

# Location of clock ports
set_property LOC IOB_X1Y124 [get_ports USER_CLK]

# Clock net "cpu_clk_g" driven by instance "cpu_clk_buf" located at site "BUFGCTRL_X0Y16"
#startgroup
create_pblock {CLKAG_cpu_clk_g}
add_cells_to_pblock [get_pblocks  {CLKAG_cpu_clk_g}] [get_cells -filter { PRIMITIVE_GROUP != I/O && IS_PRIMITIVE==1 && PRIMITIVE_LEVEL !=INTERNAL } -of_object [get_pins -filter {DIRECTION==IN} -of_objects [get_nets -hierarchical -filter {PARENT=="cpu_clk_g"}]]]
resize_pblock [get_pblocks {CLKAG_cpu_clk_g}] -add {CLOCKREGION_X0Y0:CLOCKREGION_X0Y0 CLOCKREGION_X1Y0:CLOCKREGION_X1Y0 CLOCKREGION_X1Y1:CLOCKREGION_X1Y1 CLOCKREGION_X1Y2:CLOCKREGION_X1Y2}
#endgroup

# Clock net "SCLK_OBUF_BUFG" driven by instance "SCLK_OBUF_BUFG_inst" located at site "BUFGCTRL_X0Y0"
#startgroup
create_pblock {CLKAG_SCLK_OBUF_BUFG}
add_cells_to_pblock [get_pblocks  {CLKAG_SCLK_OBUF_BUFG}] [get_cells -filter { PRIMITIVE_GROUP != I/O && IS_PRIMITIVE==1 && PRIMITIVE_LEVEL !=INTERNAL && NAME!=SCLK_OBUF_inst} -of_object [get_pins -filter {DIRECTION==IN} -of_objects [get_nets -hierarchical -filter {PARENT=="SCLK_OBUF_BUFG"}]]]
resize_pblock [get_pblocks {CLKAG_SCLK_OBUF_BUFG}] -add {CLOCKREGION_X1Y0:CLOCKREGION_X1Y0}
#endgroup
